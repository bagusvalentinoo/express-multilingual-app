{
  "name": "express-multilingual-app",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run build && NODE_ENV=production bun run ./dist/server.js",
    "build": "tsc && tsc-alias",
    "dev": "NODE_ENV=development bun run --watch ./src/server.ts",
    "test": "NODE_ENV=test bun test",
    "test:watch": "NODE_ENV=test bun --watch test",
    "test:coverage": "NODE_ENV=test bun test --coverage",
    "test:unit": "NODE_ENV=test bun test tests/unit/**/*",
    "test:unit:watch": "NODE_ENV=test bun --watch test tests/unit/**/*",
    "test:integration": "NODE_ENV=test bun test tests/integration/**/*",
    "test:integration:watch": "NODE_ENV=test bun --watch test tests/integration/**/*",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint . --fix",
    "prettier": "bunx prettier --check .",
    "prettier:fix": "bunx prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bagusvalentino/express-multilingual-app.git"
  },
  "bugs": {
    "url": "https://github.com/bagusvalentino/express-multilingual-app/issues"
  },
  "homepage": "https://github.com/bagusvalentino/express-multilingual-app#readme",
  "keywords": [
    "express",
    "multilingual",
    "app",
    "i18n",
    "multilingual"
  ],
  "author": "Bagus Valentino",
  "license": "MIT",
  "description": "Express Multilingual App",
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-rate-limit": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.9",
    "@types/supertest": "^6.0.2",
    "bun-types": "^1.2.1",
    "eslint": "^9.19.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "tsc-alias": "^1.8.10",
    "typescript-eslint": "^8.21.0"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "i18next": "^24.2.2",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.7.1",
    "morgan": "^1.10.0",
    "winston": "^3.17.0",
    "zod": "^3.24.1"
  }
}
