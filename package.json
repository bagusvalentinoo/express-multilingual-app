{
  "name": "express-multilingual-app",
  "version": "1.0.0",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "start": "bun run ./dist/server.js",
    "build": "tsc",
    "dev": "bun run --watch ./src/server.ts",
    "test": "bun test",
    "test:watch": "bun --watch test",
    "lint": "bunx eslint ./",
    "lint:fix": "bunx eslint ./ --fix",
    "prettier": "bunx prettier --check .",
    "prettier:fix": "bunx prettier --write ."
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "express",
    "multilingual",
    "app",
    "i18n",
    "multilingual"
  ],
  "author": "Bagus Valentino",
  "license": "MIT",
  "description": "Express Multilingual App",
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.10",
    "eslint": "^9.19.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-prefer-arrow-functions": "^3.6.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.21.0"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "express": "^4.21.2",
    "winston": "^3.17.0"
  }
}
